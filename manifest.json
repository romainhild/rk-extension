{
    "name": "Runkeeper-data",
    "version": "0.1",
    "description": "Get Runkeeper data",
    "icons": {
      "32": "images/running32.png",
      "48": "images/running48.png",
      "64": "images/running64.png",
      "128": "images/running128.png"
    },
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "32": "images/running32.png",
            "48": "images/running48.png",
            "64": "images/running64.png",
            "128": "images/running128.png"            
        }
    },
    "permissions": ["storage", "tabs", "https://runkeeper.com/"]
}
